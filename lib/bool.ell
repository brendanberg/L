Bool: <True|False>

True: Bool.True
False: Bool.False

Bool ('!') -> { (.True: { False }, .False: { True })(this) }

Bool ('==': b) -> { (.True: { b }, .False: { !b })(this) }
Bool ('!=': b) -> { !(this == b) }

Bool ('/\': b) -> { (.True: { b }, .False: { False })(this) }
Bool ('\/': b) -> { (.True: { True }, .False: { b })(this) }
