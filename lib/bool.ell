Bool: <True|False>

True: Bool.True
False: Bool.False

Bool (__not__) -> {(
	.True: False
	.False: True
)(this)}

Bool (__eql__: b) -> {(
	.True: b,
	.False: !b
)(this)}

Bool (__neq__: b) -> { !(this == b) }

Bool (__and__: b) -> {(
	.True: b
	.False: False
)(this)}

Bool (__or__: b) -> {(
	.True: True
	.False: b
)(this)}
